{
	"name": "sailfroum",
	"version": "0.0.0",
	"private": true,
	"husky": {
		"hooks": {
			"pre-commit": "npm run lint-staged"
		}
	},
	"lint-staged": {
		"*.js": [
			"eslint --fix",
			"git add"
		],
		"*.+(js|jsx|ts|tsx|json|css|less|scss|md|html)": [
			"prettier --write",
			"git add"
		]
	},
	"scripts": {
		"start": "set NODE_ENV=prod&&nodemon ./bin/www",
		"dev": "set NODE_ENV=dev&&nodemon ./bin/www",
		"format": "eslint . && prettier --write \"./*.+(js|jsx|ts|tsx|json|css|less|scss|md|html)\" \"./**/*.+(js|jsx|ts|tsx|json|css|less|scss|md|html)\""
	},
	"dependencies": {
		"cookie-parser": "~1.4.4",
		"cors": "^2.8.5",
		"crypto": "^1.0.1",
		"debug": "~2.6.9",
		"express": "~4.16.1",
		"express-jwt": "^8.4.1",
		"http-errors": "~1.6.3",
		"jade": "~1.11.0",
		"jsonwebtoken": "^9.0.2",
		"knex": "^3.1.0",
		"lodash": "^4.17.21",
		"moment": "^2.30.1",
		"morgan": "~1.9.1",
		"mysql": "^2.18.1",
		"node-uuid": "^1.4.8",
		"nodemon": "^3.1.0",
		"redis": "^4.6.13"
	},
	"devDependencies": {
		"@eslint/js": "^9.3.0",
		"eslint": "^9.3.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"globals": "^15.3.0",
		"husky": "^9.0.11",
		"lint-staged": "^15.2.2",
		"prettier": "^3.2.5"
	}
}
